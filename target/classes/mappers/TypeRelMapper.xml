    <result column="siteTypeId" jdbcType="INTEGER" property="sitetypeid" />
  </resultMap>
  <insert id="insert" parameterType="com.dreamcc.gs.bean.TypeRel">
    insert into site_type_rel (siteId, siteTypeId)
    values (#{siteid,jdbcType=INTEGER}, #{sitetypeid,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.dreamcc.gs.bean.TypeRel">
    insert into site_type_rel
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="siteid != null">
        siteId,
      </if>
      <if test="sitetypeid != null">
        siteTypeId,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="siteid != null">
        #{siteid,jdbcType=INTEGER},
      </if>
      <if test="sitetypeid != null">
        #{sitetypeid,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>